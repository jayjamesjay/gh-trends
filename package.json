    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.2"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupEnzyme.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test && yarn lint",
